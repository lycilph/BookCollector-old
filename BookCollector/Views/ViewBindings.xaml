<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:screen_view_models="clr-namespace:BookCollector.ViewModels.Screens"
                    xmlns:data_view_models="clr-namespace:BookCollector.ViewModels.Data"
                    xmlns:screen_views="clr-namespace:BookCollector.Views.Screens"
                    xmlns:data_views="clr-namespace:BookCollector.Views.Data">
    <!-- Screens -->
    <DataTemplate DataType="{x:Type screen_view_models:ImportScreenViewModel}">
        <screen_views:ImportScreenView/>
    </DataTemplate>

    <!-- Data -->
    <DataTemplate DataType="{x:Type data_view_models:ImportBookViewModel}">
        <data_views:ImportBookView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type data_view_models:ImportShelfViewModel}">
        <data_views:ImportShelfView/>
    </DataTemplate>
</ResourceDictionary>